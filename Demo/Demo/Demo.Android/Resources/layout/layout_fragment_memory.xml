<?xml version="1.0" encoding="utf-8" ?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:fitsSystemWindows="true">

  <GridLayout
    android:id="@+id/grid_layout_vectors"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:weightSum="99"
    android:gravity="center"
    android:rowCount="3"
    android:columnCount="3">

    <LinearLayout
      android:id="@+id/linear_layout_1"
      android:gravity="center"
      android:layout_weight="33"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content">

      <ImageView
        android:id="@+id/vector_image_view_1"
        app:srcCompat="@drawable/android"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/contentPadding"
        android:layout_marginBottom="@dimen/contentPadding"
        android:layout_marginLeft="@dimen/sidePadding"
        android:layout_marginRight="@dimen/sidePadding" />

    </LinearLayout>


    <LinearLayout
      android:id="@+id/linear_layout_2"
      android:gravity="center"
      android:layout_weight="33"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content">

      <ImageView
        android:id="@+id/vector_image_view_2"
        app:srcCompat="@drawable/android"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/contentPadding"
        android:layout_marginBottom="@dimen/contentPadding"
        android:layout_marginLeft="@dimen/sidePadding"
        android:layout_marginRight="@dimen/sidePadding" />

    </LinearLayout>

    <LinearLayout
      android:id="@+id/linear_layout_3"
      android:gravity="center"
      android:layout_weight="33"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content">

      <ImageView
        android:id="@+id/vector_image_view_3"
        app:srcCompat="@drawable/android"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/contentPadding"
        android:layout_marginBottom="@dimen/contentPadding"
        android:layout_marginLeft="@dimen/sidePadding"
        android:layout_marginRight="@dimen/sidePadding" />

    </LinearLayout>

    <LinearLayout
      android:id="@+id/linear_layout_4"
      android:gravity="center"
      android:layout_weight="33"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content">

      <ImageView
        android:id="@+id/vector_image_view_4"
        app:srcCompat="@drawable/android"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/contentPadding"
        android:layout_marginBottom="@dimen/contentPadding"
        android:layout_marginLeft="@dimen/sidePadding"
        android:layout_marginRight="@dimen/sidePadding" />

    </LinearLayout>

    <LinearLayout
      android:id="@+id/linear_layout_5"
      android:gravity="center"
      android:layout_weight="33"
